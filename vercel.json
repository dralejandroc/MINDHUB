{
  "version": 2,
  "builds": [
    {
      "src": "mindhub/frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/mindhub/frontend/$1"
    }
  ],
  "functions": {
    "mindhub/frontend/app/**/*.ts": {
      "runtime": "@vercel/node"
    },
    "mindhub/frontend/app/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}