{
  "version": 2,
  "builds": [
    {
      "src": "clinimetrix_django/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "clinimetrix_django/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "python manage.py collectstatic --noinput",
  "devCommand": "python manage.py runserver",
  "outputDirectory": ".",
  "framework": null
}