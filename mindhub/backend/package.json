{
  "name": "mindhub-backend",
  "version": "1.0.0",
  "description": "MindHub Healthcare Platform - Backend API Services",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:dev": "node server-dev.js",
    "start:simple": "node server-simple.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "docs:generate": "node shared/docs/generate-docs.js",
    "docs:watch": "nodemon --watch shared/routes --watch shared/middleware --exec \"npm run docs:generate\"",
    "docs:serve": "npx http-server docs -p 8080 -c-1 --cors",
    "docs:build": "npm run docs:generate && npm run docs:optimize",
    "docs:optimize": "node shared/docs/optimize-docs.js",
    "test:api": "node shared/testing/test-runner.js --filter api --verbose",
    "test:integration": "node shared/testing/test-runner.js --filter integration --verbose",
    "test:compliance": "node shared/testing/test-runner.js --filter compliance --verbose",
    "test:performance": "node shared/testing/test-runner.js --filter performance --verbose",
    "test:all": "node shared/testing/test-runner.js --verbose",
    "test:ci": "node shared/testing/test-runner.js --format junit,json --no-verbose",
    "universal:migrate": "node scripts/run-migrations.js",
    "universal:reset": "node scripts/reset-database.js",
    "universal:stats": "node scripts/dev-tools.js db-stats",
    "universal:add-scale": "node scripts/add-scale.js",
    "universal:list-scales": "node scripts/dev-tools.js list-scales",
    "universal:dev": "node scripts/dev-tools.js",
    "universal:backup": "node scripts/dev-tools.js backup-db",
    "universal:clean": "node scripts/dev-tools.js clean",
    "universal:bulk-add": "node scripts/bulk-add-scales.js",
    "universal:bulk-add-auto": "node scripts/bulk-add-scales.js --auto-migrate",
    "data:check": "node check-database-status.js",
    "data:create-minimal": "node create-minimal-test-data.js",
    "data:create-full": "node create-test-data.js",
    "sync-scales": "node scripts/sync-templates.js",
    "sync-scale": "node scripts/sync-templates.js"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "archiver": "^6.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.5",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-slow-down": "^2.1.0",
    "express-validator": "^7.2.1",
    "firebase-admin": "^12.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.5.0",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mammoth": "^1.9.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.1",
    "prisma": "^5.6.0",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.0",
    "react-dropzone": "^14.3.8",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "twilio": "^4.19.3",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "eslint": "^8.54.0",
    "eslint-config-node": "^4.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mindhub/healthcare-platform.git"
  },
  "keywords": [
    "healthcare",
    "mental-health",
    "api",
    "nodejs",
    "express",
    "prisma",
    "mysql"
  ],
  "author": "MindHub Healthcare Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mindhub/healthcare-platform/issues"
  },
  "homepage": "https://github.com/mindhub/healthcare-platform#readme"
}
