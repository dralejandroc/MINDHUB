{
  "name": "mindhub",
  "version": "1.0.0",
  "description": "MindHub - SaaS platform for mental health professionals",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy": "gcloud app deploy",
    "deploy:staging": "gcloud app deploy --version staging",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "auth0:setup": "node scripts/setup-auth0.js",
    "auth0:test": "node scripts/test-auth0.js",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "db:seed": "node scripts/seed-database.js",
    "db:reset": "npx prisma migrate reset --force",
    "db:init": "npm run db:generate && npm run db:push && npm run db:seed",
    "xampp:setup": "node scripts/setup-xampp.js",
    "xampp:test": "node scripts/setup-xampp.js --test",
    "xampp:init": "npm run xampp:setup && npm run db:generate && npm run db:push && npm run db:seed",
    "cloudsql:setup": "node scripts/setup-cloud-sql.js",
    "cloudsql:test": "node -e \"require('./backend/shared/config/cloud-sql').testSecureConnection().then(console.log)\"",
    "cloudsql:info": "node -e \"require('./backend/shared/config/cloud-sql').getConnectionInfo().then(console.log)\"",
    "firestore:setup": "node scripts/setup-firestore-formx.js",
    "firestore:init": "node -e \"require('./backend/shared/database/firestore/formx-schema').initializeCollections().then(console.log)\"",
    "firestore:test": "node -e \"console.log('Firestore connection test - implement in setup script')\"",
    "storage:test": "node scripts/test-storage.js",
    "storage:init": "node -e \"require('./backend/shared/config/storage').initializeLocalStorage()\"",
    "storage:stats": "node -e \"require('./backend/shared/config/storage').getStorageStats().then(console.log)\""
  },
  "keywords": [
    "healthcare",
    "mental-health",
    "clinimetrix",
    "expedix",
    "formx",
    "resources",
    "auth0",
    "google-cloud"
  ],
  "author": "MindHub Development Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/sql": "^3.0.0",
    "@google-cloud/storage": "^6.10.1",
    "@google-cloud/kms": "^3.2.0",
    "@google-cloud/firestore": "^6.8.0",
    "@prisma/client": "^6.11.1",
    "auth0": "^4.0.1",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-oauth-server": "^2.0.0",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.15.0",
    "firebase-admin": "^11.8.0",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.2",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-auth0": "^1.4.3",
    "pg": "^8.11.0",
    "prisma": "^6.11.1",
    "uuid": "^9.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
